public class Main
{
    public static void main(String[] args) {
        
        
        String firstName = "Joe";
        String lastName = "Smith";
        String middleName = "Mark";
        
        char firstInitial = 'J';
        char middleInitial = 'M';
        char lastInitial = 'S';
        
        
        System.out.print(firstName + " " + middleName + " " + lastName + " " + firstInitial + middleInitial + lastInitial);
        
    }
}
