
/*
chapter 2 challenge 6

rounding up to second decimal 
*/
 import java.text.DecimalFormat;
 
public class Main
{
    public static void main(String[] args) {
        
        double acres = 43560;
        double track = 389767;
        double result = 0;
        
        result = track / acres;
        
        DecimalFormat df = new DecimalFormat("#,###.00");
        String formatResult = df.format(result);
        
        System.out.print("Total acres in the track are: " + formatResult);
        
    }
}
